@prefix : <http://example/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

:me a foaf:Person;
    foaf:knows [
      a foaf:Person;
      foaf:knows :him
    ];
    foaf:knows [
      a foaf:Person;
      foaf:knows :you;
      foaf:knows :me
].

:you a foaf:Person;
  foaf:knows :me.

:him a foaf:Person.

